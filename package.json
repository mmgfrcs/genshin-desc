{
  "name": "nuxt-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "strapi": "npm run develop --prefix cms",
    "strapi:export": "npm run strapi export --prefix cms -- --no-encrypt --exclude config",
    "strapi:export-all": "npm run strapi export --prefix cms -- --no-encrypt",
    "release": "commit-and-tag-version"
  },
  "devDependencies": {
    "@kevinmarrec/nuxt-pwa": "^0.17.0",
    "@nuxt/content": "^2.5.2",
    "@nuxt/image-edge": "^1.0.0-28020728.5df24eb",
    "@nuxtjs/color-mode": "^3.2.0",
    "@nuxtjs/google-fonts": "^3.0.0",
    "@nuxtjs/i18n": "npm:@nuxtjs/i18n-edge",
    "@nuxtjs/tailwindcss": "^6.6.5",
    "@pinia/nuxt": "^0.4.7",
    "@types/animejs": "^3.1.7",
    "commit-and-tag-version": "^11.2.1",
    "date-fns": "^2.29.3",
    "floating-vue": "^2.0.0-beta.20",
    "nuxt": "^3.4.1",
    "nuxt-icon": "^0.3.3",
    "pinia": "^2.0.33"
  },
  "overrides": {
    "vue": "latest"
  },
  "dependencies": {
    "animejs": "^3.2.1",
    "flowbite": "^1.6.5"
  },
  "commit-and-tag-version": {
    "header": "Genshin Descriptor Changelog",
    "types": [
      {"type": "feat", "section": "Features"},
      {"type": "fix", "section": "Bug Fixes"},
      {"type": "chore", "hidden": true},
      {"type": "docs", "hidden": true},
      {"type": "style", "hidden": true},
      {"type": "refactor", "hidden": true},
      {"type": "perf", "hidden": true},
      {"type": "test", "hidden": true},
      {"type": "init", "hidden": true}
    ]
  }
}
